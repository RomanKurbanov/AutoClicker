<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="237.0" prefWidth="175.0">
         <children>
            <Button fx:id="commenceButton" defaultButton="true" layoutX="49.0" layoutY="138.0" mnemonicParsing="false" onAction="#onCommenceButton" prefHeight="22.0" prefWidth="79.0" text="Commence" textAlignment="CENTER">
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
            <TextField fx:id="howManyTimes" layoutX="15.0" layoutY="31.0" prefHeight="23.0" prefWidth="148.0" promptText="Times" text="10">
               <font>
                  <Font size="12.0" />
               </font>
            </TextField>
            <Slider fx:id="latency" blockIncrement="5.0" layoutX="15.0" layoutY="84.0" min="1.0" minorTickCount="5" prefHeight="15.0" prefWidth="148.0" showTickMarks="true" value="1.0" />
            <Label layoutX="39.0" layoutY="63.0" text="Clicks per second">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <Label layoutX="41.0" layoutY="14.0" text="How many clicks">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <CheckBox fx:id="isInfinite" layoutX="56.0" layoutY="110.0" mnemonicParsing="false" text="Infinite" />
            <Button fx:id="stopButton" cancelButton="true" disable="true" layoutX="48.0" layoutY="171.0" mnemonicParsing="false" onAction="#stop" prefHeight="22.0" prefWidth="79.0" text="Stop" textAlignment="CENTER">
               <font>
                  <Font size="11.0" />
               </font>
            </Button>
            <ProgressBar fx:id="pBar" layoutX="17.0" layoutY="203.0" prefHeight="20.0" prefWidth="148.0" progress="0.0" />
         </children>
      </Pane>
   </children>
</GridPane>
